{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:recommended",
        "group:allNonMajor",
        ":semanticCommitTypeAll(chore)"
    ],
    "meteor": {
        "enabled": false
    },
    "rangeStrategy": "bump",
    "schedule": [
        "every weekend"
    ],
    "assigneesFromCodeOwners": true
}